/*订单中心  */
.order-center{
  width: 100%;
  height: calc(100vh - 86rpx);
}

.swiper-tab{  
  display: flex;
  width: 100%;  
  border-bottom: 2rpx solid #777777;  
  border-top:1px solid #ebebeb;
  text-align: center;  
  line-height: 80rpx;
}  
.swiper-tab-list{  
  flex:1;
  font-size: 30rpx;  
  display: inline-block;  
  color: #777777;  
}  
.on{ 
  color: #055b58; 
  border-bottom: 2rpx solid #055b58;
}  
.swiper-box{ 
  display: block; 
  height:calc(100vh - 86rpx);
  width: 100%; 
  overflow: hidden; 
  background-color:#fafafa;
}  
.swiper-box view{  
  text-align: center;  
  height:calc(100vh - 82rpx);
}  
.no-order-img{
  display: block;
  width: 227rpx;
  height: 256rpx;
  margin: 100rpx auto;
}
.order-list{
  display:block;
  margin-top: 10rpx;
  background-color:#ffffff;
}
.head{
  display: flex;
  height: 80rpx;
  line-height: 80rpx;
  align-items: center;
  padding: 0 30rpx;
  overflow: hidden;
  background-color: #ffffff;
  margin-top: 10rpx;
  font-family: PingFang-SC;
  font-size: 30rpx;
  text-align: left;
  color: #333333;
}
.business-img{
  display: block;
  min-width: 36rpx;
  max-width: 36rpx;
  height: 37rpx;
}
.business{
  flex: 1;
  text-indent: 10rpx;
}
.car-module{
  display: flex;
  width:calc(100% - 30rpx);
  padding-left: 30rpx;
  height: 200rpx;
  margin-bottom:10rpx;
  align-items: center;
  background-color: #fafafa;
}
.order-goods{
  display:flex;
  overflow:hidden;
  width: 100%;
  align-items:center;
}
.car-module:last-child{
  margin-bottom: 0;
}
.car-good{
  display: block;
  width: 180rpx;
  height: 180rpx;
  overflow: hidden;
}
.car-detail{
  display: block;
  flex:1;
  height: 200rpx;
  padding-left: 20rpx;
  padding-right:30rpx;
  font-family: PingFang-SC;
  font-size: 30rpx;
  text-align: left;
  color: #333333;
  line-height:40rpx;
}
.good-name{
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow:hidden;
  min-height: 80rpx;
}
.spec{
  font-size: 24rpx;
  text-align: left;
  color: #999999;
}
.item-data{
  display: flex;
  line-height:80rpx;
}
.item-data .price{
  display: block;
  flex: 1;
  color: #ff2929;
  font-weight: 500;
}
.item-data .num{ 
  display: block;
  flex: 1;
  font-weight: 500;
  color: #999999;
  text-align: right;
}
.status{
  font-size: 24rpx;
  text-align: right;
  color: #ff2929;
}
.footer{
  display:block;
  padding-left: 30rpx;
  background-color: #ffffff;
  font-family:PingFang-SC;
  font-size:30rpx;
  line-height: 90rpx;
  text-align:right;
  color:#333333;
}
.total{
  display: block;
  text-align: right;
  padding-right: 30rpx;
  border-bottom: 1rpx solid #e5e5e5;
}
.handle{
  display: flex;
  justify-content: flex-end;
  align-items:center;
  padding-right:30rpx;
  height: 90rpx;
}
.order-btn{
  display: block;
  width: 140rpx;
  height: 48rpx;
  margin-left:30rpx;
  line-height: 48rpx;
  border-radius: 6rpx;
  border: 1rpx solid #cccccc;
  text-align: center;
}
.order-suc{
  border: 1rpx solid #055b58;
  color: #055b58;
}
/*遮罩层  */
.shade{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.7);
  z-index:2;
}
.confirm{
  position: absolute;
  top: 30%;
  left: 50%;
  margin-left: -280rpx;
  display: block;
  width: 560rpx;
  height: 280rpx;
  background-color: #ffffff;
  border-radius:10rpx;
  overflow: hidden;
}
.content{
  display: block;
  height: 80rpx;
  line-height: 80rpx;
  padding: 50rpx 30rpx;
  font-family: PingFang-SC;
  font-size: 30rpx;
  text-align: center;
  color: #333333;
}
.btn{
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  font-family: PingFang-SC;
  font-size: 34rpx;
  text-align: center;
  color: #333333;
  border-top: 1rpx solid #e5e5e5;
}
.btn p{
  display: block;
  flex: 1;
}
.btn p:nth-child(2){
  background-color: #055b58;
  color: #ffffff;
}
/*支付密码  */
.pay-password{
  position: absolute;
  top: 120rpx;
  left: 50%;
  margin-left: -280rpx;
  width: 560rpx;
  height: 400rpx;
  border-radius: 10rpx;
  background-color: #ffffff;
  overflow: hidden;
}
.pay-title{
  position: relative;
  display:block;
  height: 80rpx;
  line-height: 80rpx;
  font-family: PingFang-SC;
  font-size: 30rpx;
  text-align: center;
  color: #333333;
  border-bottom: 1rpx solid #e5e5e5;
}
.pay-title p{
  display: block;
}
.close1-img{
  position: absolute;
  top: 26rpx;
  left: 30rpx;
  width: 27rpx;
  height: 27rpx;
}
.pay-price{
  display: block;
  padding: 40rpx 30rpx;
  font-family: PingFang-SC;
  font-size: 26rpx;
  text-align: center;
  color: #333333;
}
.pay-price span{
  display: block;
}
.pay-price span:nth-child(2){
  font-size: 60rpx;
}
.pwd-frame{
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding:0 20rpx;
}
.pwd-frame p{
  display: flex;
  flex: 1;
  max-width: 80rpx;
  height: 80rpx;
  border: 1rpx solid #cccccc;
  align-items: center;
  justify-content: center;
}
.pwd-frame p span{
  display: block;
  width: 36rpx;
  height: 36rpx;
  background-color: #000000;
  border-radius: 50%;
}
.pwd-key{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-wrap: wrap;
  background-color: #ffffff;
}
.pwd-key p{
  display: flex;
  width: 33%;
  height: 110rpx;
  line-height: 110rpx;
  border-right: 1rpx solid #cccccc;
  border-bottom: 1rpx solid #cccccc;
  font-size: 50rpx;
  color: #333;
  text-align: center;
  justify-content: center;
  align-items: center;
}
.bg-e6{
  background-color:#E6E6E6;
}
.del-img{
  width: 50rpx;
  height: 50rpx;
}