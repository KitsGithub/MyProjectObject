.classify{
  display: flex;
  width: 100%;
  height: 100%;
  background-color: #fafafa;
}
.class-left{
  display: block;
  width: 192rpx;
  height: calc(100vh - 10rpx);
  font-size: 30rpx;
  background-color: #ffffff;
  margin-bottom: 10rpx;
}
.type{
  display: block;
  width: 192rpx;
  height: 90rpx;
  line-height: 90rpx;
  color: #333333;
  text-align: center;
  background-color: #ffffff;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.type-active{
  color: #ffffff;
  background-color: #055b58;
}
.class-right{
  display: flex;
  width: 100%;
  height: calc(100vh - 20rpx);
  flex-wrap:wrap;
  margin-left: 10rpx;
  margin-top: 10rpx;
}
.class{
  display: flex;
  width: calc(100vw - 202rpx);
  height: 100vh;
  background-color: #ffffff;
  flex-wrap:wrap;
}
.good{
  background-color: #ffffff;
}
.good-img{
  display: block;
  width: 182rpx;
  height: 182rpx;
  overflow: hidden;
}
.good-name{
  display: block;
  width: 182rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-family: PingFang-SC;
  font-size: 26rpx;
  text-align: center;
  color: #999999;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
