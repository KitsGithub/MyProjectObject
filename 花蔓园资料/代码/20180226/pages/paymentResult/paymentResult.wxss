/*支付结果  */
.pay-result{
  width: 100%;
  height: 100vh;
  background-color: #ffffff;
  font-family: PingFang-SC;
  font-size: 30rpx;
  text-align: center;
  color: #333333;
}
.result-img{
  display:block;
  margin:200rpx auto 20rpx;
  width: 216rpx;
  height: 216rpx;
}
.pay-result p{
  display: block;
  line-height:50rpx;
}
.result-btn{
  display: block;
  color:#055b58;
  width:300rpx;
  height:80rpx;
  line-height: 80rpx;
  margin:80rpx auto;
  border-radius:6rpx;
  border:solid 2rpx #055b58;

}
